@{
    ViewBag.Title = "Detay";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@model IEnumerable< AsansorTakipV3.Models.YapilanIs>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-6">
            <h1 class="page-header"> Devam Eden İş Detayları</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="form-group">
                        <a class="btn btn-primary" href="@Url.Action("IsEkle", "YapilanIs", new {Id=@ViewBag.Tut})">
                            İş Ekle
                        </a>
                    </div>
                    <table width="100" class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <tr>
                            <th>Kayit Tarihi</th>
                            <th>Açıklama</th>
                            <th>Resim</th>
                            <th>Maliyet</th>
                            <th>Sil</th>
                        </tr>
                        @foreach (var item in Model)
                        {
                        <tr class="odd gradeX">
                            <td>@item.Tarih</td>
                            <td>@item.Aciklama</td>
                            <td>@item.Resim</td>
                            <td>@item.Maliyet</td>
                            @*<td><a href="@Url.Action("HizmetDetay", "Musteriler", new { Id = item.Id , Musteri=item.Musteri_Id})"><i class="icon icon-edit"></i></a></td>
                                <td><a href="@Url.Action("HizmetDetay", "Musteriler", new { Id = item.Id , Musteri=item.Musteri_Id})"><i class="icon icon-tools"></i></a></td>
                                <td><a href="@Url.Action("HizmetSil", "Musteriler", new { Id = item.Id })"><i class="icon icon-delete"></i></a></td>*@
                        </tr>
                        }
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
